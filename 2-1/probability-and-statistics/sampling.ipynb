    "import numpy as np\n",
    "count = 0\n",
    "n = int(1e6)\n",
    "for i in range(n):\n",
    "    sample = [np.random.normal(800,300) for j in range(16)]\n",
    "    if sum(sample) > 16*900:\n",
    "        count += 1\n",
    "print(count/n)"
   ]
